// From https://github.com/GoogleChrome/puppeteer/issues/1193#issuecomment-400125070

const puppeteer = require('puppeteer');

const URL = 'https://googlechrome.github.io/samples/service-worker/basic/';

async function main() {
  const browser = await puppeteer.launch({
    //args: ['--enable-features=NetworkService'],
    //headless: false,
    //devtools: true,
  });
  const page = await browser.newPage();
  await page.setRequestInterception(true);
  page.on('request', req => req.continue());
  await page.goto(URL);
}

main().then(data => console.log(data)).catch(err => console.error(err));
